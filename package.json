{
  "name": "fcc-markdown-parser",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@tailwindcss/postcss7-compat": "^2.0.2",
    "marked": "^1.2.7",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.1",
    "styled-components": "^5.2.1",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.0.2",
    "web-vitals": "^0.2.4"
  },
  "devDependencies": {
    "@craco/craco": "^6.0.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.3",
    "@testing-library/user-event": "^12.6.0",
    "autoprefixer": "^9.8.6",
    "babel-plugin-prismjs": "^2.0.1",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "postcss": "^7.0.35",
    "prettier": "^2.2.1",
    "twin.macro": "^2.0.8"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babelMacros": {
    "twin": {
      "config": "./src/tailwind.config.js",
      "preset": "styled-components"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  }
}
